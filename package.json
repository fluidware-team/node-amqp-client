{
  "name": "@fluidware-it/amqp-client",
  "version": "0.0.0",
  "main": "build/src/index.js",
  "module": "build/esm/index.js",
  "esnext": "build/esnext/index.js",
  "types": "build/src/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "author": "Fluidware srl",
  "license": "Apache-2.0",
  "scripts": {
    "prepack": "npm run compile",
    "compile": "npm run clean && tsc --build tsconfig.json tsconfig.esm.json tsconfig.esnext.json",
    "clean": "tsc --build --clean tsconfig.json tsconfig.esm.json tsconfig.esnext.json",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "test": "jest",
    "env2doc": "npm run compile && env2doc -p build/src > ENVIRONMENT.md",
    "preversion": "npm run lint && npm run test && npm run env2doc"
  },
  "engines": {
    "node": ">=18.4"
  },
  "dependencies": {
    "@fluidware-it/saddlebag": "^0.1.1",
    "amqplib": "^0.10.3"
  },
  "devDependencies": {
    "@fluidware-it/env2doc": "^0.3.1",
    "@types/amqplib": "^0.10.4",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "files": [
    "build/esm/**/*.js",
    "build/esm/**/*.js.map",
    "build/esm/**/*.d.ts",
    "build/esnext/**/*.js",
    "build/esnext/**/*.js.map",
    "build/esnext/**/*.d.ts",
    "build/src/**/*.js",
    "build/src/**/*.js.map",
    "build/src/**/*.d.ts",
    "LICENSE",
    "README.md"
  ]
}
